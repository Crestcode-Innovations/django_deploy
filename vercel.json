{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
    "outputDirectory": ".",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ],
    "projectSettings": {
        "rootDirectory": "backend"
    }
}